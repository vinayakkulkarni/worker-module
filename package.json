    "@nuxtjs/eslint-config": "latest",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/test-utils": "latest",
    "ava": "latest",
    "babel-eslint": "latest",
    "babel-plugin-module-resolver": "latest",
    "browser-env": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "latest",
    "jsdom-global": "latest",
